<div class="text-start org-page">
    <aside>
      <button class="btn btn-primary"
      data-bs-toggle="collapse" href="#orgCreation" role="button" 
      aria-expanded="false" aria-controls="orgCreation">
        <span>Create Organization</span>
        <span>
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-plus-circle" viewBox="0 0 16 16">
            <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14m0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16"/>
            <path d="M8 4a.5.5 0 0 1 .5.5v3h3a.5.5 0 0 1 0 1h-3v3a.5.5 0 0 1-1 0v-3h-3a.5.5 0 0 1 0-1h3v-3A.5.5 0 0 1 8 4"/>
          </svg>
        </span>
      </button>

      <div class="collapse" id="orgCreation">
          <%- include('../complete-setup') %>
      </div>

    </aside>


    <div class="table-responsive small">
      <table class="table">
        <caption>List of Organizations</caption>
        <thead>
          <tr>
            <th scope="col">Org Name</th>
            <th scope="col">Description</th>
            <th scope="col">Total Size</th>
            <th scope="col">Active No. of Members</th>
            <th scope="col"></th>
          </tr>
        </thead>
        <tbody>
  
        <% orgs.forEach((item) => {%>
          <tr>
            <th scope="row"><a href=""><%=  item.name %></a></th>
            <td><%=  item.description %></td>
            <td><%=  item.population %></td>
            <td>15</td>
            <td><button>Edit</button>
            <button>Delete</button></td>
          </tr>
  
        <% }); %>
  
                
        </tbody>
      </table>
  
  </div>

  
 
</div>